{
  "builds": [
    {
      "src": "FSD-1/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "FSD-2/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "FSD-3/theme_toggle_spa/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "FSD-4/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/fsd-1/(.*)", "dest": "/FSD-1/$1" },
    { "src": "/fsd-2/(.*)", "dest": "/FSD-2/$1" },
    { "src": "/fsd-3/(.*)", "dest": "/FSD-3/theme_toggle_spa/$1" },
    { "src": "/fsd-4/(.*)", "dest": "/FSD-4/$1" },
    { "src": "/", "dest": "/FSD-1/index.html" }
  ]
}
